<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    >

    <com.hzn.easytableview.EasyTableView
        android:id="@+id/table"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#cacaca"
        android:padding="20dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        custom:etvBgColor="#9e9f3b"
        custom:etvRows="2"
        custom:etvLines="1"
        custom:etvMode="normal"
        custom:etvHeaderHColor="#008e85d4"
        custom:etvHeaderVColor="#008e85d4"
        custom:etvHeaderHVColor="#b33737"
        custom:etvOutStrokeCorner="10dp"
        custom:etvOutStrokeSize="6dp"
        custom:etvOutStrokeColor="#00d3c2"
        custom:etvStrokeSize="2dp"
        custom:etvStrokeColor="#000000"
        />
</RelativeLayout>
